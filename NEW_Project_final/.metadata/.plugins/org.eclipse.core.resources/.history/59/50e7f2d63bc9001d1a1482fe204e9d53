package testBoard.board.dto;

public class DayPriceDTO {
	
		private int num;
		private String price;
		
		public DayPriceDTO() {
		}

		public DayPriceDTO(int num, String price) {
			this.num = num;
			this.price = price;
		}

		public int getNum() {
			return num;
		}

		public void setNum(int num) {
			this.num = num;
		}

		public String getPrice() {
			return price;
		}

		public void setPrice(String price) {
			this.price = price;
		}
	
		
		
}
