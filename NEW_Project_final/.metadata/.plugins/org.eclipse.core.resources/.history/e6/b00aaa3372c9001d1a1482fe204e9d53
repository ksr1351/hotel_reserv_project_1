package testBoard.board.dto;

import java.util.Date;

public class PayPageDTO {
	
	
	private int room_code;
	private int hotel_code;
	
	private String name;
	private String room_name;
	private String room_price;
	
	private Date checkin;
	private Date checkout;
	
	private int days;
	private String totalPrice;
	
	public PayPageDTO() {
	
	}

	public PayPageDTO(int room_code, int hotel_code, String name, String room_name, String room_price, Date checkin,
			Date checkout, int days, String totalPrice) {
		this.room_code = room_code;
		this.hotel_code = hotel_code;
		this.name = name;
		this.room_name = room_name;
		this.room_price = room_price;
		this.checkin = checkin;
		this.checkout = checkout;
		this.days = days;
		this.totalPrice = totalPrice;
	}

	
	
	
	
	
}
